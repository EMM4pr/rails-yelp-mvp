<div>
<h1>Your list of restaurants</h1>
  <div id="restaurants">
    <% @restaurants.each do |restaurant| %>
    <div class="restaurant">
        <%= render restaurant %>
    </div>
    <% end %>

  </div>

<div class="btn">
  <%= link_to "+ New restaurant", new_restaurant_path, class:"button" %>
</div>
